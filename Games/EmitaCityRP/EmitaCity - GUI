--==========================================================================================--

--GUI Options Folder
local theme = venyx:addPage("GUI", 5012544693)
local hud1 = theme:addSection("Options")

--Toogle GUI
hud1:addKeybind("Toggle GUI Keybind", Enum.KeyCode.One, function()
	print("Activated Keybind")
	venyx:toggle()
end, function()
	print("Changed Keybind")
end)

--Change Colors
for theme, color in pairs(themes) do -- all in one theme changer, i know, im cool
	hud1:addColorPicker(theme, color, function(color3)
		venyx:setTheme(theme, color3)
	end)
end


hud1:addButton("Destroy",function()
    game:GetService("CoreGui"):FindFirstChild("Venyx"):Destroy()
end)
--==========================================================================================--

-- load
venyx:SelectPage(venyx.pages[1], true)
