-- init
local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/RedBlurredX/ROBLOX/main/ScriptDevelopment/Ui%20Library/Venyx%20-%20By%3A%20GreenDeno/Source.lua"))()
local venyx = library.new("Emita City RP - Made by: RedBlurred", 5013109572)

-- themes
local themes = {
	Background = Color3.fromRGB(24, 24, 24),
	Glow = Color3.fromRGB(0, 0, 0),
	Accent = Color3.fromRGB(10, 10, 10),
	LightContrast = Color3.fromRGB(20, 20, 20),
	DarkContrast = Color3.fromRGB(14, 14, 14),  
	TextColor = Color3.fromRGB(255, 255, 255)
}


--==========================================================================================--

--Player Folder
local plr = venyx:addPage("Player", 5012544693)
local section1 = plr:addSection("Player Options")
local section2 = plr:addSection("Others")

--PVP MODE
section1:addButton("Enable PVP",function()
    local args = {
    [1] = true
    }
    game:GetService("ReplicatedStorage").PVP:InvokeServer(unpack(args))
end)

section1:addButton("Disable PVP",function()
    local args = {
    [1] = false
    }
    game:GetService("ReplicatedStorage").PVP:InvokeServer(unpack(args))
end)

--FriendlyMode
section1:addButton("FriendlyMode",function()
    game:GetService("ReplicatedStorage").FriendlyMode:FireServer()
end)

--Infinity yield
section2:addButton("Infinite Yield", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)

--Aimbot
section2:addButton("Aimbot",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/RedBlurredX/ROBLOX/main/Aimbots/Aimbot%20(Best).lua"))()
end)
--UnnamedESP
section2:addButton("ESP / WallHack",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/RedBlurredX/ROBLOX/main/Universal/ESP/UnnamedESP"))()
end)

--ChatLogs Console
section2:addButton("ChatLogger (CONSOLE)",function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/RedBlurredX/ROBLOX/main/Universal/Chat%20Log%20-%20Console.lua"))()
end)


--==========================================================================================--

--Money Folder
local moneypage = venyx:addPage("Money", 5012544693)
local money1 = moneypage:addSection("Options")
local money2 = moneypage:addSection("Printers")
money1:addButton("Deposit All", function()
        local args = {
        [1] = "All"
    }
    game:GetService("ReplicatedStorage").Remotes.BankSystem:FireServer(unpack(args))
end)

money1:addTextbox("WithDraw", "Default", function(value)
        local args = {
        [1] = "WithDraw",
        [2] =  (value)
    }
    game:GetService("ReplicatedStorage").Remotes.BankSystem:FireServer(unpack(args))
end)

money1:addTextbox("Deposit", "Default", function(value)
        local args = {
        [1] = "Deposit",
        [2] =  (value)
    }
    game:GetService("ReplicatedStorage").Remotes.BankSystem:FireServer(unpack(args))
end)

--Give Printer
money2:addButton("Give Printer",function()
    game:GetService("ReplicatedStorage").Events.storePrinter:FireServer()
end)


--==========================================================================================--

--Gun Mods Folder
local guns = venyx:addPage("Weapons", 5012544693)
local gunswarn = guns:addSection("Put your weapon on Hotbar before use it!")
local guns1 = guns:addSection("Select Weapon")
local guns2 = guns:addSection("Gun Mods")

--Select Weapon
local gunmod = nil
guns1:addDropdown("Pistols", {"M1911", "Makarov", "M9", "Glock", "MP443", "Revolver", "Desert"}, function(gunname)
    gunmod = require(game.Players.LocalPlayer.Backpack[(gunname)].Setting)
        print("Weapon Selected=", (gunname))
end)

guns1:addDropdown("Semi", {"Uzi", "UMG", "MAC 10"}, function(gunname)
    gunmod = require(game.Players.LocalPlayer.Backpack[(gunname)].Setting)
        print("Weapon Selected=", (gunname))
end)

guns1:addDropdown("Shotgun", {"Shorty", "M1100"}, function(gunname)
    gunmod = require(game.Players.LocalPlayer.Backpack[(gunname)].Setting)
        print("Weapon Selected=", (gunname))
end)

guns1:addDropdown("Rifles", {"AK47", "M4A1", "Mosin", "Mini 14", }, function(gunname)
    gunmod = require(game.Players.LocalPlayer.Backpack[(gunname)].Setting)
        print("Weapon Selected=", (gunname))
end)

guns1:addDropdown("Snipers", {"Dragunov", "Mini14"}, function(gunname)
    gunmod = require(game.Players.LocalPlayer.Backpack[(gunname)].Setting)
        print("Weapon Selected=", (gunname))
end)

--Weapon Weapons
guns2:addSlider("Custom Damage", 0, 0, 500, function(value)
    gunmod.BaseDamage = (value)
        print("Gun Successful edited!! OH YEAH")
end)

guns2:addButton("INF Damage", function()
    gunmod.BaseDamage = math.huge
        print("Gun Successful edited!! OH YEAH")
end)

guns2:addButton("INF Ammo", function()
    gunmod.AmmoPerMag = math.huge
        print("Gun Successful edited!! OH YEAH")
end)

guns2:addButton("No Reload", function()
    gunmod.ReloadTime = 0
        print("Gun Successful edited!! OH YEAH")
end)

guns2:addButton("RapidFire (Need NoReload)", function()
    gunmod.FireRate = 0.5
        print("Gun Successful edited!! OH YEAH")
end)


